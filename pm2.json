{
  "name": "hackernews",
  "script": "./main.js",
  "cwd": "./",
  "watch": [
    "dist",
    "routers"
  ],
  "ignore_watch": [
    "node_modules",
    "logs",
    "public"
  ],
  "watch_options": {
    "followSymlinks": false
  },
  "error_file": "./logs/app-err.log",
  "out_file": "./logs/app-out.log",
  "env": {
    "NODE_ENV": "production",
    "PORT": "8007"
  }
}